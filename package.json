{
    "name": "lunatic-mui",
    "version": "0.0.1",
    "description": "Library of questionnaire components using Material UI",
    "repository": {
        "type": "git",
        "url": "git://github.com/ddecrulle/lunatic-mui.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "echo DODO",
        "build": "tsc",
        "lint:check": "eslint . --ext .ts,.tsx",
        "lint": "npm run lint:check -- --fix",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/ddecrulle",
    "license": "MIT",
    "files": [
        "src/",
        "!src/test/",
        "dist/",
        "!dist/test/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [
        "react",
        "component",
        "react-component",
        "library",
        "material-ui"
    ],
    "homepage": "https://github.com/ddecrulle/lunatic-mui",
    "devDependencies": {
        "@babel/core": "^7.18.13",
        "@inseefr/lunatic": "^0.4.5-v2",
        "@storybook/addon-actions": "^6.5.10",
        "@storybook/addon-essentials": "^6.5.10",
        "@storybook/addon-interactions": "^6.5.10",
        "@storybook/addon-links": "^6.5.10",
        "@storybook/builder-webpack5": "^6.5.10",
        "@storybook/manager-webpack5": "^6.5.10",
        "@storybook/react": "^6.5.10",
        "@storybook/testing-library": "^0.0.13",
        "@types/node": "^16.4.9",
        "@types/react": "^18.0.17",
        "@typescript-eslint/eslint-plugin": "^4.28.5",
        "@typescript-eslint/parser": "^4.28.5",
        "babel-loader": "^8.2.5",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-storybook": "^0.6.4",
        "eslint-plugin-tss-unused-classes": "^0.0.4",
        "husky": "^4.3.8",
        "lint-staged": "^11.1.1",
        "prettier": "^2.3.2",
        "storybook-dark-mode": "^1.1.0",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "@emotion/react": "^11.10.4",
        "@emotion/styled": "^11.10.4",
        "@mui/material": "^5.10.3",
        "disiz": "^0.0.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tss-react": "^4.0.0"
    }
}
